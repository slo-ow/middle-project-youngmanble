            <Image url="@../images/내용.PNG" />
         </image>
      </ImageView>
      <TextField fx:id="txtName" layoutX="210.0" layoutY="109.0" prefHeight="23.0" prefWidth="137.0" />
      <TextArea fx:id="txtContents" layoutX="210.0" layoutY="222.0" prefHeight="215.0" prefWidth="459.0" />
      <TextField fx:id="txtTitle" layoutX="210.0" layoutY="169.0" prefHeight="23.0" prefWidth="319.0" />
      <Button fx:id="applyBtn" layoutX="355.0" layoutY="516.0" mnemonicParsing="false" onAction="#apply" style="-fx-background-color: #000000; -fx-border-color: #ffffff;" text="등록" textFill="WHITE" />
      <Button fx:id="canBtn" layoutX="405.0" layoutY="516.0" mnemonicParsing="false" onAction="#cancel" style="-fx-background-color: #000000; -fx-border-color: #ffffff;" text="취소" textFill="WHITE" />
   </children>
</Pane>
